<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="mobile_api_cadastro_clientes.Editar">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtNome" Placeholder="Nome" />
            <Entry x:Name="txtSobrenome" Placeholder="Sobrenome"/>
            <Entry x:Name="txtIdade" Placeholder="Idade" />
            <Button Text="Adicionar" Clicked="Adicionar" FontSize="Small"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>